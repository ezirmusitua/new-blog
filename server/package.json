{
  "name": "koa2-blog-server",
  "version": "0.0.1",
  "description": "Blog server",
  "main": "index.ts",
  "scripts": {
    "start": "tsc & babel -d ./dist ./dist & node ./dist/index.js",
    "deploy": "tsc & babel -d ./dist ./dist & NODE_ENV=prod node ./dist/index.js",
    "build": "tsc & babel -d ./dist ./dist",
    "test": "jest"
  },
  "dependencies": {
    "@types/kcors": "^2.2.0",
    "@types/koa": "^2.0.37",
    "@types/koa-bodyparser": "^3.0.22",
    "@types/koa-router": "^7.0.21",
    "@types/mongoose": "^4.7.2",
    "koa": "^2.0.0-alpha.7",
    "kcors": "^2.2.0",
    "koa-bodyparser": "^3.1.0",
    "koa-router": "^7.0.1",
    "mongoose": "^4.7.6",
    "typescript": "2.2.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.14.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-core": "^6.14.0",
    "babel-cli": "^6.23.0"
  },
  "devDependencies": {
    "@types/jest": "^18.1.1",
    "babel-cli": "^6.23.0",
    "babel-jest": "^18.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-nodemon": "^2.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-tslint": "^6.1.1",
    "gulp-typescript": "^3.1.3",
    "jest": "^18.1.0",
    "nodemon": "^1.11.0",
    "ts-jest": "^18.0.3",
    "tslint": "^4.2.0",
    "yargs": "^6.5.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__specs__/.*|\\.(spec))\\.ts$",
    "moduleFileExtensions": [
      "ts"
    ]
  },
  "keywords": [
    "koa2"
  ],
  "author": "jferroal@gmail.com",
  "license": "MIT"
}
